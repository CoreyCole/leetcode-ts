{
  "name": "leetcode-ts",
  "description": "Repository for leetcode problem solutions written in Typescript",
  "version": "0.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/CoreyCole/leetcode-ts"
  },
  "dependencies": {
    "typescript": "2.5.3",
    "typescript-collections": "1.2.5"
  },
  "devDependencies": {
    "@types/jest": "21.1.4",
    "@types/node": "8.0.47",
    "jest": "21.2.1",
    "nodemon": "1.12.1",
    "ts-jest": "21.1.3",
    "tslint": "5.7.0",
    "tslint-config-standard": "6.0.1"
  },
  "scripts": {
    "test": "jest",
    "test-watch": "jest --watchAll",
    "clean": "rm -rf ./build",
    "lint": "tslint --project .",
    "compile": "tsc -p ."
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testMatch": [
      "**/spec.ts"
    ]
  }
}
